<body>
  <main>
    <h1>Registro</h1>
    <form action="/api/session/register" method="post">
      <label for="nombre-cliente">Nombre del usuario</label><br />
      <input type="text" name="first_name" id="first_name" placeholder="Nombre" required /><br />
      <label for="last_name">Apellido</label><br />
      <input type="text" name="last_name" id="last_name" placeholder="Apellido" required /><br />
      <label for="email">Email</label><br />
      <input type="email" name="email" id="email" placeholder="Correo electronico" required />
      <br />
      <label for="age">edad</label><br />
      <input type="number" name="age" id="age" placeholder="Edad" min="0" required />
      <br />
      <label for="password">Contrasena</label><br />
      <input type="password" name="password" id="password" placeholder="introduzca su contrasena" required />
      <br />
      <button type="submit">Registrar</button>
    </form>
  </main>
</body>